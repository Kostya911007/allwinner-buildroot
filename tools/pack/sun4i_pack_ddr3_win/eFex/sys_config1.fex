card_high_speed = 1
card_line       = 4
SDC_CMD		 	= port:PC6<3><1>
SDC_CLK	    	= port:PC7<3><1>
SDC_D0		 	= port:PC8<3><1>
SDC_D1		 	= port:PC9<3><1>
SDC_D2		 	= port:PC10<3><1>
